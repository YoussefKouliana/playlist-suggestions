const person1 = {
    name: "Anthony Martial",
    ageRange: "25-35"
}

const person2 = {
    name: "Franseca",
    ageRange: "65-75",
}

function SuggestGoodMusic(person){
    if (person.ageRange === "25-35"){
        console.log("we think you will like Jazz musik")
    } else if (person.ageRange === "65-75") {
        console.log("Johny cash is a good option")
    } else { console.log("Justin Bieber is the best")
}
}
SuggestGoodMusic(person1)
SuggestGoodMusic(person2)
